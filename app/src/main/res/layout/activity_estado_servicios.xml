<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/activity_menu_principal"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.usuario.webesdi.EstadoServicios"
tools:layout_editor_absoluteY="81dp"
tools:layout_editor_absoluteX="0dp">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentTop="true"
    android:layout_alignParentStart="true">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/btncantina"
            android:layout_width="129dp"
            android:layout_height="129dp"
            app:srcCompat="@drawable/cantina_verd"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/btnContacto"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textWifi" />
        <ImageView
            android:id="@+id/btnGoogleGmail"
            android:layout_width="129dp"
            android:layout_height="129dp"
            app:srcCompat="@drawable/gmail_verd"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/btnContacto"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textWifi" />
        <ImageView
            android:id="@+id/btnGoogleCalendar"
            android:layout_width="129dp"
            android:layout_height="129dp"
            app:srcCompat="@drawable/calendari_verd"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/btnContacto"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textWifi" />



        <ImageView
            android:id="@+id/btnwifi"
            android:layout_width="129dp"
            android:layout_height="129dp"
            app:srcCompat="@drawable/wifi_verd"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/btnimpresoras"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/txtTitulo"/>

        <ImageView
            android:id="@+id/btnimpresoras"
            android:layout_width="129dp"
            android:layout_height="129dp"
            app:srcCompat="@drawable/impresora_verda"
            android:layout_marginLeft="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/txtTitulo" />

        <TextView
            android:id="@+id/textimpresoras"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="0dp"
            android:text="@string/menuSimpresores"
            android:textColor="#707070"
            android:textSize="24sp"
            app:layout_constraintLeft_toLeftOf="@+id/btnimpresoras"
            app:layout_constraintRight_toRightOf="@+id/btnimpresoras"
            app:layout_constraintTop_toBottomOf="@+id/btnimpresoras"
            app:layout_constraintHorizontal_bias="0.285" />

        <ImageView
            android:id="@+id/btnGoogleDocs"
            android:layout_width="129dp"
            android:layout_height="129dp"
            app:srcCompat="@drawable/docs_verd"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textimpresoras"
            android:layout_marginLeft="24dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/textWifi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/menuSwifi"
            android:textColor="#707070"
            android:textSize="24sp"
            app:layout_constraintLeft_toLeftOf="@+id/btnimpresoras"
            app:layout_constraintRight_toRightOf="@+id/btnimpresoras"
            app:layout_constraintTop_toBottomOf="@+id/btnimpresoras"
            />

        <TextView
            android:id="@+id/textContacto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:text="@string/menuP_contacto"
            android:textColor="#707070"
            android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.428"
            app:layout_constraintLeft_toLeftOf="@+id/btnContacto"
            app:layout_constraintRight_toRightOf="@+id/btnContacto"
            android:layout_marginBottom="18dp"
            app:layout_constraintBottom_toTopOf="@+id/btnMensajes" />


        <TextView
            android:id="@+id/txtEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/txtTitulo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="18sp"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/txtEmail"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>
</RelativeLayout>